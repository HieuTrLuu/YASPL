start{
  seen = [];
  index = [];

}
0-1{
  return ($0);
  seen = [$0];
  index = [1];
}

1-2{
  return ($0);
  seen = seen ++ [$0];
  index = index ++ [2];
}

2+{
  seen = seen++[$0];
  buffer = (last index) +1;
  index = index ++ [buffer];

  buffer2 = $0 + head(tail (tail (reverse seen)));
  return buffer2;
}
